<?xml version='1.0' encoding='utf-8'?>
<widget id="com.makeapp.bizmanager" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>BizManager</name>
    <description>An application created for YummyYummy.co.il</description>
    <author email="roy@makeapp.co.il" href="https://www.makeapp.co.il/">
		MakeAPP
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="orientation" value="landscape" />
</widget>
